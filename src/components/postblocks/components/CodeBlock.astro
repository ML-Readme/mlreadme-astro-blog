---
import type { Props as $, Mark as MarkType } from "astro-portabletext/types";
import { Code } from "astro:components";

export type Props = $<MarkType>;

const props = Astro.props

const textCode = props.node.children.map( line => {
    return line.text
}).join('')

console.log(props.node.children)
---
<Code code={textCode} lang="python"></Code>