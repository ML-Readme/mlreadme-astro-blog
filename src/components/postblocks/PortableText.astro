---
import { PortableText as PortableTextInternal } from 'astro-portabletext'
import PortableTextImage from "./PortableImage.astro";
import CustomBlock from './components/CustomBlock.astro';
import { urlForImage } from "@sanitysrc/lib/sanityUrl.ts";

const components = {
  type: {
    image: PortableTextImage,
    block: CustomBlock
  }
};

const { portableText } = Astro.props;
---

<PortableTextInternal value={portableText} components={components}/>